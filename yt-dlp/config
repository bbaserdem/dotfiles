# Set default search option to youtube
--default-search "youtube1:"
--yes-playlist
--paths "temp:/tmp"
--paths "home:${HOME}/Downloads/"
--output "%(title)s-%(resolution)s.%(ext)s"
--restrict-filenames
--no-overwrites
--audio-format 'best'
--audio-quality 0
--write-subs
--no-write-auto-subs
--sub-lang "en,tr"
--alias get-audio,-X '--extract-audio --prefer-free-formats --format-sort abr,aext --audio-quality 0 --merge-output-format ogg'
